{
  "type": "project",
  "name": "kecoyo/turtle-admin",
  "license": "MIT",
  "homepage": "https://github.com/kecoyo/turtle-admin",
  "description": "Application development framework",
  "keywords": [
    "ThinkAdmin",
    "ThinkLibrary"
  ],
  "authors": [{
    "name": "kecoyo",
    "email": "kecoyo@163.com"
  }],
  "require": {
    "php": ">=7.1.0",
    "ext-gd": "*",
    "ext-xml": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "endroid/qr-code": "^1.9",
    "topthink/framework": "^6.0",
    "topthink/think-view": "^1.0",
    "zoujingli/ip2region": "^1.0",
    "zoujingli/think-library": "6.0.x-dev",
    "zoujingli/wechat-developer": "^1.2"
  },
  "autoload": {
    "psr-0": {
      "": "extend"
    },
    "psr-4": {
      "app\\": "app"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php think service:discover",
      "@php think vendor:publish"
    ]
  },
  "minimum-stability": "stable",
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer"
    }
  }
}
